					<Add option="-pg" />
					<Add option="-g" />
					<Add option="-DDEFINED_BUILD_MODE_PRIVATE" />
				</Compiler>
				<Linker>
					<Add option="-pg" />
				</Linker>
				<Environment>
					<Variable name="BUILD_MODE" value="-DDEFINED_BUILD_MODE_PRIVATE" />
				</Environment>
			</Target>
			<Target title="Release">
				<Option output="../PlatformerExperiment" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-w" />
					<Add option="-D__NO_INLINE__" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<Environment>
					<Variable name="BUILD_MODE" value="" />
				</Environment>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add directory="../SDL2Stuff/include" />
		</Compiler>
		<Linker>
			<Add library="mingw32" />
			<Add library="SDL2main" />
			<Add library="SDL2" />
			<Add library="SDL2_ttf" />
			<Add library="SDL2_image" />
			<Add directory="../SDL2Stuff/lib" />
		</Linker>
