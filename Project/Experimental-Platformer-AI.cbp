<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Experimental-Platformer-AI" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option show_notes="0">
			<notes>
				<![CDATA[Use Complie-Final.bat for full releases of the program. It is smaller than the one Code Blocks makes and the version infomation shows up properly on that one.]]>
			</notes>
		</Option>
		<Build>
			<Target title="Debug">
				<Option output="../PlatformerExperiment-PRIVATE" prefix_auto="1" extension_auto="1" />
